# Salin ke .env lalu sesuaikan nilai untuk production

APP_NAME=Tafta
APP_ENV=production
APP_KEY=base64:...  # Generate: php artisan key:generate --show
APP_DEBUG=false
APP_URL=https://your-domain.com

DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=tafta
DB_USERNAME=tafta
DB_PASSWORD=your_strong_password

# Port yang di-expose ke host (default 80)
APP_PORT=80
